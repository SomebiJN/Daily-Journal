<%- include('partials/header'); -%>
<h1>Home</h1>
<p><%= homeContent %></p>

<% Newposts.forEach(post => { %>

    <h1><%= post.title %></h1>
    <% post.content %>
    <p>
        <%= post.content.substring(0, 100) + "..." %> 
        <a href="/posts/<%= post.title %>">Read More</a>
    </p>

<%});%>
    
<%- include('partials/footer'); -%>